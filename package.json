  },
  "homepage": "https://github.com/TechLadies/onesg-app#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "knex": "^0.21.6",
    "morgan": "^1.10.0",
    "objection": "^2.2.3",
    "pg": "^8.4.2",
